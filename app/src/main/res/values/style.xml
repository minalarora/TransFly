<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RobotoTextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/poppinsmedium</item>
        <item name="textAllCaps">false</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#8c8c8c</item>
    </style>

    <style name="iconGenText">
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">@font/font_opensans</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="textblack" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/black</item>

    </style>
    <style name="textwhite" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/white</item>

    </style>

</resources>